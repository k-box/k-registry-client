{
  "name": "oneofftech/k-link-registry-client",
  "type": "library",
  "description": "Client library for K-Registry API",
  "keywords": ["oneofftech", "k-search", "k-registry"],
  "license": "AGPL-3.0-only",
  "require": {
    "php": ">=7.1",
    "php-http/httplug": "^1.0",
    "php-http/client-common": "^1.1",
    "php-http/discovery": "^1.0",
    "php-http/message-factory": "^1.0",
    "php-http/message": "^1.0",
    "guzzlehttp/psr7": "^1.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.8",
    "php-http/curl-client": "^1.6",
    "phpstan/phpstan": "^0.8.5",
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "OneOffTech\\KLinkRegistryClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "OneOffTech\\KLinkRegistryClient\\Tests\\": "tests/"
    }
  }
}
